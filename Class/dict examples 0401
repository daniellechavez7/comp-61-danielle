students = {}
students ["Jose"] = {"Grade": "A+", "StudentID": 88445}
students ["Shawn"] = {"Grade": "F", "StudentID": 88674}

print(students)
print("Jose:")

print(f' Grade: {students ["Jose"]["Grade"]}')
print(f' ID: {students["Jose"]["StudentID"]}')

