car_dict = {}
car_dict['bmw series 3'] = 30000
car_dict['honda civic'] = 26000
car_dict['kia forte'] = 24000
car_dict['toyota camry'] = 29000


print(car_dict)

for value in car_dict:
    print(value)

for key, value in car_dict.items():
    print(key)
    print(value)

for key in car_dict.keys():
    print(key)

for key, value in car_dict.items():
    if ( key == "bmw series 3" ):
        print(value)


students = {}
students ["Jose"] = {"Grade": "A+", "StudentID": 88445}
students ["Shawn"] = {"Grade": "F", "StudentID": 88674}
students ['Bella'] = {"Grade": 'F', 'StudentID': 88273}
students ['River'] = {"Grade": 'F', 'StudentID': 88028}
students ['Bea'] = {'Grade': 'A+', 'StudentID': 88456}
students ['Jay'] = {'Grade': 'F', 'StudentID': 88246}


for key, value in students.items():
    for info_key, info_value in value.items():
        if (info_value == 'F'):
            print(key)
            print(value)


print(students)
print(students['Shawn'])
students.clear()
print(students)
print(students.get('Shawn'))


class studentInfo: 
    def __init__(self, _grade, _student_id):
        self.grade = 'F'
        self.student_id = '-1'

student_class_dict = {}
student_class_dict ['Jose'] = studentInfo('A+')

