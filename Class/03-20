
class Student(): 
    def __init__(self, _name, _age):
        self.name = _name 
        self.age = _age 
    def drinking (self):
        if self.age >= 21: 
            print("Okay here's a beer")
        else: 
            print("You are not of age")

students = []

a = 1
if (a < 3):         #only check first condition 
    print("hello")
elif (a < 5):
    print("hi")
elif (a < 2):
    print("oh")
else:
    print ("bye")
if (a > 0):         #each if statement resets checking logic 
    print("haha")
if (a >= 1):
    print("lol")

a = 1 
b = a < 3 # gives us value of True or False
if (b): 
    print("haha")
if (False): 
    print("heyhey")
if (True):
    print("hihi")

a = 4 
while (a > 0): 
    a = a - 1
    print(a) 
while(True): 
    a = a - 1 
    print(a)
    if (a < 0): 
        break
while(False):
    a = a - 1 
    print(a)


