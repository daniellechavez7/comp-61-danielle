print(range(3))
for i in range (3):
    print(i)

l = [4, 5, 6]
for i in l:
    print (i)

index = 0 
for i in range(len(l)):
    index = index + 1
    print(i, index)

d = {"first": 7, "second": 8, "third": 9}
for i in d: 
    print (i)

for key, value in d.items():
    print(key, value)
    #print(value)

for key in d.keys():
    print(key)

for value in d.values():
    print(value)

